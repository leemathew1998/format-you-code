{
  "name": "format-vue-style-code",
  "displayName": "format-vue-style-code",
  "description": "format„ÄÅsort you vue2 code",
  "version": "0.0.5",
  "icon": "icon.png",
  "publisher": "leemathew1998",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:vue"
  ],
  "repository": {
    "url": "https://github.com/leemathew1998/format-you-code"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "format-you-code.file",
        "title": "Format Vue Style Code"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == vue",
          "command": "format-you-code.file",
          "group": "1_modification"
        }
      ]
    },
    "keybindings": [
      {
        "command": "format-you-code.file",
        "when": "resourceLangId == vue",
        "key": "ctrl+shift+alt+f",
        "mac": "cmd+shift+alt+f"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "cssnano": "^5.1.14",
    "postcss": "^8.4.21",
    "vue-template-compiler": "^2.7.14"
  }
}
